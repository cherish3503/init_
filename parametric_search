def upper_bound(arr, value):
  # sort된 배열 arr
  # arr 배열의 원소 중 조건을 만족하는 최대값을 구함
  low, high = 0, len(arr)-1

  result = arr[0]
  

  while low <= high:
      mid = (low + high) // 2
      if check(arr[mid]):
          low = mid + 1
          result = arr[mid]
          # 조건을 만족하는 경우 값을 result에 대입
      else:
          high = mid - 1
  return result
  
  def lower_bound(arr, value):
  # sort된 배열 arr
  # arr 배열의 원소 중 조건을 만족하는 최솟값을 구함
  low, high = 0, len(arr)-1

  result = arr[0]
  

  while low <= high:
      mid = (low + high) // 2
      if check(arr[mid]):
          high = mid - 1
          result = arr[mid]
          # 조건을 만족하는 경우 값을 result에 대입
      else:
          low = mid + 1
          
  return result
  
  def check(n):
      return True
  
  
